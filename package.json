{
	"name": "js-intellisense",
	"displayName": "js-intellisense",
	"description": "Integrates js-intellisense into VS Code.",
	"version": "1.4.6",
	"author": "shannonliang",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/shannonliang/vscode-js-intellisense.git"
	},
	"bugs": {
		"url": "https://github.com/shannonliang/vscode-js-intellisense/issues"
	},
	"publisher": "shannonliang",
	"icon": "intell.png",
	"categories": [
		"Languages"
	],
	"keywords": [
		"multi-root ready"
	],
	"engines": {
		"vscode": "^1.16.0"
	},
	"activationEvents": [
		"*"
	],
	"main": "./client/out/extension",
	"contributes": {

	},
	"scripts": {
		"watch:client": "tsc -w -p ./client/tsconfig.json",
		"watch:server": "tsc -w -p ./server/tsconfig.json",
		"compile": "npm run compile:client && npm run compile:server",
		"postinstall": "cd client && npm install && cd ../server && npm install && cd .."
	},
	"devDependencies": {
		"typescript": "^2.7.1",
		"@types/node": "^6.0.90"
	}
}
